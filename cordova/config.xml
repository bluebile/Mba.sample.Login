<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.mbamobi.exemplo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>exemplo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" version="4.1.0" />
    <engine name="android" version="5.1.1" />
    <engine name="wp8" version="https://github.com/bluebile/cordova-wp8.git" />
    <engine name="blackberry10" version="3.7.0" />

    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.2" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />

    <feature name="ExitApp">
        <param name="id" value="se.sanitarium.cordova.exitapp" />
        <param name="url" value="https://github.com/bluebile/cordova-plugin-exitapp.git" />
    </feature>
</widget>
